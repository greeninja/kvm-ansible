<network>                            
  <name>{{ item.name }}</name>
  <bridge name="virbr{{ item.id }}"/>
  <forward mode="nat"/>
  <domain name="{{ item.dnsname }}"/>
  <ip address="{{ item.ipaddress }}" netmask="{{ item.subnet }}">
    <dhcp>
      <range start="{{ item.dhcp_start }}" end="{{ item.dhcp_end }}"/>
    </dhcp>
  </ip>   
</network>
